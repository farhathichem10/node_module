    "@progress/kendo-schematics": "^3.0.0",
    "tslib": "^2.3.1"
  },
  "peerDependencies": {
    "@angular/animations": "12 - 15",
    "@angular/common": "12 - 15",
    "@angular/core": "12 - 15",
